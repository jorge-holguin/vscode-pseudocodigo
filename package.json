{
    "name": "pseudocodigo",
    "displayName": "Pseudocodigo",
    "description": "Una extensión para resaltar la sintaxis del pseudocodigo en español",
    "publisher": "jorgeholguin",
    "icon": "images/icon.png",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pseudocode",
                "aliases": [
                    "Generic Pseudocode",
                    "pseudocode"
                ],
                "extensions": [
                    ".psc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pseudocode",
                "scopeName": "source.pseudo",
                "path": "./syntaxes/pseudocode.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "pseudocode",
                "path": "./snippets.json"
            }
        ],
        "icons": {
            "fileIcons": [
                {
                    "iconPath": "./icons/psc-icon.png",
                    "fileExtensions": ["psc"]
                }
            ]
        }
    },
    "activationEvents": [
        "onLanguage:pseudocode"
    ],
    "main": "./out/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/jorge-holguin/vscode-pseudocodigo"
    },
    "devDependencies": {
        "@types/node": "^15.0.1",
        "@types/vscode": "^1.55.0",
        "typescript": "^4.2.4",
        "vscode": "^1.1.37"
    },
    "dependencies": {}
}
